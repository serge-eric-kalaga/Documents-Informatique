button { border: 0px; border-radius: 4px; box-sizing: border-box; color: var(--primary-button-text-color); cursor: pointer; float: right; font-size: 0.875em; margin: 0px; padding: 8px 16px; transition: box-shadow 150ms cubic-bezier(0.4, 0, 0.2, 1) 0s; user-select: none; }

