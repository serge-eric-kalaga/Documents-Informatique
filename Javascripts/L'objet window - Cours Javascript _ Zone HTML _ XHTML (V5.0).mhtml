#cookie-bar p { margin: 0px; padding: 5px 0px 0px; font-weight: bolder; font-size: 12px; }

#cookie-bar a { color: rgb(255, 255, 255); display: inline-block; border-radius: 3px; text-decoration: none; padding: 0px 6px; margin-left: 8px; }

#cookie-bar .cb-enable { background: rgb(246, 144, 0); }

#cookie-bar .cb-enable:hover { background: rgb(246, 144, 0); }

#cookie-bar .cb-disable { background: rgb(153, 0, 0); }

#cookie-bar .cb-disable:hover { background: rgb(187, 0, 0); }

#cookie-bar .cb-policy { background: rgb(237, 168, 70); }

#cookie-bar .cb-policy:hover { background: rgb(237, 168, 70); }
