.boxRecherche ul.mediasSociaux li.facebook { background: url("//lefaso.net/squelettes-dist/img/bg_mediasSociaux.gif") 0px 0px no-repeat; }

.boxRecherche ul.mediasSociaux li.twitter { background: url("//lefaso.net/squelettes-dist/img/bg_mediasSociaux.gif") -32px 0px no-repeat; }

.boxRecherche ul.mediasSociaux li.google { background: url("//lefaso.net/squelettes-dist/img/bg_mediasSociaux.gif") -64px 0px no-repeat; }

.boxRecherche ul.mediasSociaux li.mobile { background: url("//lefaso.net/squelettes-dist/img/bg_mediasSociaux.gif") -96px 0px no-repeat; }

.boxRecherche ul.mediasSociaux li.rss { background: url("//lefaso.net/squelettes-dist/img/bg_mediasSociaux.gif") -128px 0px no-repeat; padding: 0px; }

.boxRecherche ul.mediasSociaux li.facebook a, .boxRecherche ul.mediasSociaux li.twitter a, .boxRecherche ul.mediasSociaux li.google a, .boxRecherche ul.mediasSociaux li.mobile a, .boxRecherche ul.mediasSociaux li.rss a { display: block; width: 21px; height: 0px; padding: 21px 0px 0px; overflow: hidden; }

.boxRecherche ul.mediasSociaux li.facebook a:hover { background: url("//lefaso.net/squelettes-dist/img/bg_mediasSociaux.gif") 0px -30px no-repeat; }

.boxRecherche ul.mediasSociaux li.twitter a:hover { background: url("//lefaso.net/squelettes-dist/img/bg_mediasSociaux.gif") -32px -30px no-repeat; }

.boxRecherche ul.mediasSociaux li.google a:hover { background: url("//lefaso.net/squelettes-dist/img/bg_mediasSociaux.gif") -64px -30px no-repeat; }

.boxRecherche ul.mediasSociaux li.mobile a:hover, body.mobile .boxRecherche ul.mediasSociaux li.mobile a { background: url("//lefaso.net/squelettes-dist/img/bg_mediasSociaux.gif") -96px -30px no-repeat; }

.boxRecherche ul.mediasSociaux li.rss a:hover, body.rss .boxRecherche ul.mediasSociaux li.rss a { background: url("//lefaso.net/squelettes-dist/img/bg_mediasSociaux.gif") -128px -30px no-repeat; }

.left_events { width: 100%; background-color: rgb(228, 228, 228); margin-bottom: 30px; color: rgb(92, 81, 78); line-height: 16px; }

.left_events .node { color: rgb(255, 255, 255); text-transform: uppercase; font-family: K2D; letter-spacing: 3px; font-size: 12px; width: 100%; height: 21px; line-height: 21px; background: url("//lefaso.net/squelettes-dist/img/E20-right-ext.gif") right top no-repeat; }

.left_events .node a { color: rgb(55, 55, 54); text-align: left; }

.left_events .content { margin-top: 0px; padding: 10px 0px; font-size: 14px; width: 100%; color: rgb(92, 81, 78); text-align: left; margin-bottom: 0px; border: none; line-height: 20px; }

.left_events a { font-family: Arial; letter-spacing: 1px; font-size: 14px; color: rgb(92, 81, 78); }

.cel_sponsor { float: left; margin-right: 15px; min-height: 90px; line-height: 16px; background-color: rgb(228, 228, 228); padding: 10px 5px 5px; font-size: 10px; }

.cel_sponsor_l { display: inline; float: left; }

.cel_sponsor_r { display: inline; float: right; }

.cel_sponsor td { font-size: 9px; }

.container { max-width: 1100px; }

#first_line_navigation { width: 100%; height: 26px; background-image: url("//lefaso.net/squelettes-dist/img/first-line_background.png"); background-repeat: repeat-x; margin: 0px auto; padding: 0px; font-size: 11px; letter-spacing: 2px; }

#first_line_navigation ul { margin: 0px; padding: 0px; list-style: none; }

#first_line_navigation .mlink { position: relative; display: block; float: left; height: 26px; line-height: 26px; margin-right: 0px; z-index: 1; padding-left: 10px; padding-right: 10px; border-left: 1px solid rgb(100, 114, 98); }

#first_line_navigation .mlink a { color: rgb(255, 255, 255); height: 26px; line-height: 26px; text-align: center; text-decoration: none; }

#first_line_navigation .mlink a:hover, #first_line_navigation .mlink a:focus, #first_line_navigation .mlink a:active { background: rgb(3, 23, 2); height: 26px; line-height: 26px; }

.comment_line { width: 100%; min-height: 40px; margin-bottom: 5px; padding-left: 5px; background-color: rgb(255, 255, 255); clear: both; }

.comment_line_c1 { width: 15%; min-height: 40px; line-height: 16px; padding-right: 5px; float: left; text-align: left; display: flex; align-items: center; }

.comment_line_c2 { width: 20%; min-height: 40px; line-height: 16px; padding-right: 5px; float: left; text-align: left; color: gray; display: flex; align-items: center; }

.comment_line_c3 { width: 65%; min-height: 40px; line-height: 16px; padding-right: 10px; float: left; display: flex; align-items: center; }

.ticker { font-family: K2D; letter-spacing: 1px; }

.ticker div { display: inline-block; overflow-wrap: break-word; }

#menu-icon { width: 40px; height: 40px; background: url("squelettes-dist/img/menu-icon.png") center center rgb(76, 143, 236); }

a:hover#menu-icon { background-color: rgb(1, 66, 18); border-radius: 4px 4px 0px 0px; }

nav { width: 100%; height: 50px; line-height: 50px; border-top: 2px solid rgb(1, 66, 18); border-bottom: 2px solid rgb(1, 66, 18); margin-bottom: 20px; overflow: hidden; }

nav ul, nav:active ul { padding: 14px 0px 13px; list-style: none; display: flex; justify-content: center; }

nav li { float: left; height: 21px; padding: 0px 10px; line-height: 21px; letter-spacing: 1px; text-transform: uppercase; font-family: cheltenham-normal-700; font-size: 16px; }

nav a { color: rgb(1, 66, 18); }

@media screen and (max-width: 800px) {
  header { position: absolute; }
  #menu-icon { display: inline-block; }
  nav ul, nav:active ul { display: none; position: absolute; padding: 20px; background: rgb(255, 255, 255); border: 5px solid rgb(68, 68, 68); right: 20px; top: 60px; width: 50%; border-radius: 4px 0px 4px 4px; }
  nav li { text-align: center; width: 100%; padding: 10px 0px; margin: 0px; }
  nav:hover ul { display: block; }
  .sponsors { }
  .left_events { margin-left: 10px; margin-right: 10px; }
}

.fasobarometre img { margin-right: 10px; }
